{
  "name": "@halkeye/jenkins-io-components",
  "version": "0.0.1",
  "description": "Random parts shared between jenkins.io",
  "publishConfig": {
    "access": "public"
  },
  "main": "build/jio-components.cjs.js",
  "module": "build/jio-components.ejs.js",
  "type": "module",
  "browserslist": [
    "last 2 versions",
    "iOS >= 8"
  ],
  "scripts": {
    "dev": "rollup -c --watch",
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "clean": "rimraf build jio-*.{d.ts,d.ts.map,js,js.map} test/jio-*.{d.ts,d.ts.map,js,js.map} test/jio-*_test.{d.ts,d.ts.map,js,js.map} shared-styles.{d.ts,d.ts.map,js,js.map}",
    "lint": "npm run lint:eslint && npm run lint:css",
    "lint:eslint": "eslint .",
    "lint:css": "stylelint \"src/**/*.css\"",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "docs": "npm run docs:clean && npm run build && npm run analyze && npm run docs:build && npm run docs:assets && npm run docs:gen",
    "docs:clean": "rimraf docs",
    "docs:gen": "eleventy --config=.eleventy.cjs",
    "docs:gen:watch": "eleventy --config=.eleventy.cjs --watch",
    "docs:build": "npm run build; mkdir -p docs/; cp -r build/* docs/",
    "docs:assets": "cp node_modules/prismjs/themes/prism-okaidia.css docs/",
    "docs:serve": "wds --root-dir=docs --node-resolve --watch",
    "analyze": "cem analyze --litelement --globs \"src/*.ts\"",
    "analyze:watch": "cem analyze --litelement --globs \"src/*.ts\" --watch",
    "serve": "wds --watch",
    "serve:prod": "MODE=prod npm run serve",
    "test": "npm run test:dev && npm run test:prod",
    "test:dev": "wtr",
    "test:watch": "wtr --watch",
    "test:prod": "MODE=prod wtr",
    "test:prod:watch": "MODE=prod wtr --watch",
    "test-storybook:ci": "npx concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"npx wait-on tcp:6006 && npx test-storybook --maxWorkers=2 --coverage\"",
    "semantic-release": "semantic-release",
    "storybook": "npx storybook dev -p 6006",
    "build-storybook": "npm run analyze && npx storybook build"
  },
  "keywords": [
    "jenkins-infra",
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "release": {
    "branches": [
      "main"
    ]
  },
  "author": "Gavin Mogan <npm@gavinmogan.com> (https://www.gavinmogan.com/)",
  "license": "MIT",
  "dependencies": {
    "lit": "^2.4.0",
    "outdent": "^0.8.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "@babel/core": "^7.19.3",
    "@custom-elements-manifest/analyzer": "^0.6.4",
    "@open-wc/testing": "^3.1.6",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@storybook/addon-a11y": "^7.0.0-alpha.46",
    "@storybook/addon-actions": "^7.0.0-alpha.46",
    "@storybook/addon-coverage": "^0.0.5",
    "@storybook/addon-essentials": "^7.0.0-alpha.46",
    "@storybook/addon-interactions": "^7.0.0-alpha.46",
    "@storybook/addon-links": "^7.0.0-alpha.46",
    "@storybook/addon-postcss": "^3.0.0-alpha.1",
    "@storybook/builder-webpack5": "^7.0.0-alpha.46",
    "@storybook/cli": "^7.0.0-alpha.46",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/test-runner": "^0.9.0",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/web-components": "^7.0.0-alpha.46",
    "@storybook/web-components-webpack5": "^7.0.0-alpha.46",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@web/dev-server": "^0.1.34",
    "@web/dev-server-esbuild": "^0.3.2",
    "@web/dev-server-legacy": "^1.0.1",
    "@web/rollup-plugin-html": "^1.11.0",
    "@web/rollup-plugin-polyfills-loader": "^1.3.1",
    "@web/test-runner": "^0.14.0",
    "@web/test-runner-playwright": "^0.8.10",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.5.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-lit": "^1.6.1",
    "eslint-plugin-storybook": "^0.6.6",
    "eslint-plugin-wc": "^1.3.2",
    "http-server": "^14.1.1",
    "lint-staged": "^13.0.3",
    "lit-css-loader": "^2.0.0",
    "lit-html": "^2.4.0",
    "postcss": "^8.4.17",
    "postcss-lit": "^0.5.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-execute": "^1.1.1",
    "rollup-plugin-lit-css": "^4.0.0",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-summary": "^1.4.3",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.55.0",
    "semantic-release": "^19.0.5",
    "sinon": "^14.0.1",
    "storybook": "^7.0.0-alpha.46",
    "storybook-dark-mode": "^1.1.2",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-prettier": "^2.0.0",
    "typescript": "~4.8.4",
    "wait-on": "^6.0.1",
    "webpack": "^5.74.0"
  },
  "customElements": "custom-elements.json",
  "files": [
    "build/jio-*.d.ts",
    "build/jio-*.d.ts.map",
    "build/jio-*.js",
    "build/jio-*.js.map",
    "build/shared-*.d.ts",
    "build/shared-*.d.ts.map",
    "build/shared-*.js",
    "build/shared-*.js.map",
    "custom-elements.json",
    "makeLayout.js"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run lint --"
  }
}
